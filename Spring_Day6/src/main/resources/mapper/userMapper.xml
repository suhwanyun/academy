<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="spring.dao.UserMapper">
	
	<insert id="insert" parameterType="user">
		insert into tuser (user_id, pass) values (#{userId}, #{pass}) 
	</insert>
	
</mapper>